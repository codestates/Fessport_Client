 } from './map';\nimport login, { actionWatcher } from './sign';\nimport userInfo, { userInfoSaga } from './userInfo';\nimport image, { imageSaga } from './image';\nimport boardData, { boardDataSaga } from './board';\nimport participant, { participantSaga } from './participant';\nimport commentData, { commentDataSaga } from './comment';\nimport { all, fork } from 'redux-saga/effects';\nimport festival, { festivalSaga } from './festival';\nimport artist, { artistSaga } from './artist';\nimport category, { categorySaga } from './category';\nimport wish, { wishSaga } from './wish';\n\nconst rootReducer = combineReducers({\n  map,\n  userInfo,\n  image,\n  login,\n  boardData,\n  participant,\n  commentData,\n  festival,\n  artist,\n  wish,\n  category,\n});\n\nexport function* rootSaga() {\n  yield all([\n    mapSaga(),\n    userInfoSaga(),\n    imageSaga(),\n    actionWatcher(),\n    boardDataSaga(),\n    participantSaga(),\n    commentDataSaga(),\n    festivalSaga(),\n    artistSaga(),\n    categorySaga(),\n    wishSaga(),\n  ]);\n}\n\nexport default rootReducer;\nexport type RootState = ReturnType<typeof rootReducer>;\n","/home/jason/Codestates/Fessport/Fessport_Client/src/containers/NavContainer.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/HomePage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/FessportPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/FestivalDetailPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/FestivalListPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/CompanionPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/CommunityPostPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/ResellPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/ArtistListPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/ArtistDetailPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/WishListPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/pages/ReviewPage.tsx",[],"/home/jason/Codestates/Fessport/Fessport_Client/src/containers/HomeContainer.tsx",["449"],"import React, { useEffect } from 'react';\nimport { withRouter } from 'react-router-dom';\nimport CountrySection from '../components/CountrySection';\nimport styled from 'styled-components';\n\nimport { useDispatch, useSelector } from 'react-redux';\nimport { RootState } from '../modules';\nimport { getMapDataAsync } from '../modules/map';\n\nconst HomeContainer = (): JSX.Element => {\n  const { data, loading, error } = useSelector((state: RootState) => state.map);\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (!data) {\n      console.log(111);\n      dispatch(getMapDataAsync.request());\n    }\n  }, []);\n\n  return (\n    <>\n      {data && (\n        <Temp>\n          {/* <GridImage src={'/images/gridd.png'} /> */}\n          <SubMapImage src={'/images/dots.png'} />\n          <Ttemp>\n            <MapImage src={'/images/themapp.jpg'} />\n            {/* <GridImage src={'/images/gridd.png'} /> */}\n            {data.map((country) => (\n              <CountrySection\n                key={country._id}\n                _id={country._id}\n                name={country.name}\n                y={country.y}\n                x={country.x}\n                flagImage={country.flagImage}\n                festivals={country.festivals}\n              />\n            ))}\n          </Ttemp>\n        </Temp>\n      )}\n    </>\n  );\n};\n\nconst Temp = styled.div`\n  width: 100vw;\n  height: 80vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  overflow: hidden;\n  /* margin-top: 50px; */\n`;\n\nconst SubMapImage = styled.img`\n  position: absolute;\n  width: 1250px;\n  height: 600px;\n  z-index: 98;\n`;\n\nconst Ttemp = styled.div`\n  position: relative;\n  width: 1250px;\n  height: 595px;\n`;\n\nconst MapImage = styled.img`\n  object-fit: fill;\n  position: absolute;\n  width: 1250px;\n  height: 600px;\n`;\n\nconst GridImage = styled.img`\n  position: absolute;\n  width: 1250px;\n  height: 100vh;\n`;\n\nexport default withRouter(HomeContainer);\n","/home/jason/Codestates/Fessport/Fessport_Client/src/containers/FessportContainer.tsx",["450"],"import React, { useState, useEffect } from 'react';\nimport { withRouter } from 'react-router-dom';\nimport UserInfo from '../components/UserInfo';\nimport { getUserInfoAsync } from '../modules/userInfo';\nimport Collector from '../components/Collector';\nimport Badge from '../components/Badge';\nimport styled from 'styled-components';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { RootState } from '../modules';\nconst FessportContainer = (): JSX.Element => {\n  const collectorRef: React.RefObject<HTMLDivElement> = React.createRef();\n  const { patchSucess, data, loading, error } = useSelector(\n    (state: RootState) => state.userInfo,\n  );\n  const dispatch = useDispatch();\n\n  const userInfoRef: React.RefObject<HTMLDivElement> = React.createRef();\n  const badgeRef: React.RefObject<HTMLDivElement> = React.createRef();\n\n  useEffect(() => {\n    if (!data) {\n      console.log(111);\n      dispatch(getUserInfoAsync.request());\n    } else if (patchSucess) {\n      dispatch(getUserInfoAsync.request());\n      console.log(111);\n    }\n  }, [data, patchSucess]);\n\n  const handleScrollDown = (target: string) => (\n    e: React.MouseEvent<HTMLDivElement, MouseEvent>,\n  ) => {\n    let targetLocation: HTMLDivElement;\n    switch (target) {\n      case 'collectorRef':\n        targetLocation = collectorRef.current as HTMLDivElement;\n        break;\n      case 'badgeRef':\n        targetLocation = badgeRef.current as HTMLDivElement;\n        break;\n      default:\n        targetLocation = userInfoRef.current as HTMLDivElement;\n    }\n    targetLocation.scrollIntoView({\n      behavior: 'smooth',\n      block: 'center',\n    });\n  };\n\n  return (\n    <>\n      {data && (\n        <Temp>\n          <UserInfo\n            email={data.email}\n            nickName={data.nickName}\n            image={data.image}\n            handleScrollDown={handleScrollDown}\n          />\n          <Collector\n            visits={data.visits}\n            collectorRef={collectorRef}\n            handleScrollDown={handleScrollDown}\n          />\n          <Badge badge={data.badge} badgeRef={badgeRef} />\n          <Ei />\n        </Temp>\n      )}\n    </>\n  );\n};\n\nconst Temp = styled.div`\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin-top: 1%;\n`;\n\nconst Ei = styled.div`\n  position: fixed;\n  top: 0px;\n  left: 0px;\n  width: 100vw;\n  height: 100vh;\n  z-index: -1;\n  opacity: 0.3;\n  background: radial-gradient(black 35%, transparent 1%),\n    url('/images/wall2.jpg');\n  background-size: 3px 3px, contain;\n`;\nexport default withRouter(FessportContainer);\n","/home/jason/Codestates/Fessport/Fessport_Client/src/containers/FestivalListContainer.tsx",["451","452"],"import React, { useState, useEffect } from 'react';\nimport { Link, useLocation, useHistory } from 'react-router-dom';\nimport styled from 'styled-components';\n\nimport { useDispatch, useSelector } from 'react-redux';\nimport { RootState } from '../modules';\nimport {\n  getFestivalListAsync,\n  getFestivalListMoreAsync,\n} from '../modules/festival';\nimport {\n  getFestivalCategoryAsync,\n  getCountryCategoryAsync,\n  getGenreCategoryAsync,\n} from '../modules/category';\n\ninterface IInputQuery {\n  countryId: string | null;\n  genreId: string | null;\n  search: string | null;\n}\n\nconst FestivalListContainer = (): JSX.Element => {\n  const history = useHistory();\n  const search = useLocation().search;\n  const query = new URLSearchParams(search);\n  let queryString = query.toString();\n\n  const LIMIT = 9;\n\n  const [listNumber, setListNumber] = useState(10);\n  const [offset, setOffset] = useState(0);\n  const [inputQuery, setInputQuery] = useState<IInputQuery>({\n    countryId: query.get('countryId'),\n    genreId: query.get('genreId'),\n    search: query.get('search'),\n  });\n\n  const {\n    festivalList,\n    countryCategory,\n    genreCategory,\n    festivalCategory,\n    loading,\n    error,\n  } = useSelector((state: RootState) => ({\n    festivalList: state.festival.festivalList,\n    countryCategory: state.category.country,\n    genreCategory: state.category.genre,\n    festivalCategory: state.category.festival,\n    loading: state.festival.festivalList.loading,\n    error: state.festival.festivalList.error,\n  }));\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (\n      !countryCategory.data ||\n      !genreCategory.data ||\n      !festivalCategory.data\n    ) {\n      console.log('🍗🍗🍗🍗 Category useEffect 🍗🍗🍗🍗');\n      dispatch(getFestivalCategoryAsync.request());\n      dispatch(getCountryCategoryAsync.request());\n      dispatch(getGenreCategoryAsync.request());\n    }\n  }, []);\n\n  useEffect(() => {\n    console.log('🔥🔥🔥🔥 Festival List(queryState) useEffect 🔥🔥🔥🔥');\n    query.set('offset', String(0));\n    query.set('limit', String(LIMIT));\n    queryString = query.toString();\n    setInputQuery({\n      countryId: query.get('countryId'),\n      genreId: query.get('genreId'),\n      search: query.get('search'),\n    });\n    dispatch(getFestivalListAsync.request(queryString));\n  }, [queryString]);\n\n  useEffect(() => {\n    setOffset(festivalList.data.length);\n  }, [festivalList.data]);\n\n  const handleFestivalListMore = () => {\n    console.log('✅✅✅✅ Festival List More(queryState) useEffect ✅✅✅✅');\n    query.set('offset', String(offset));\n    query.set('limit', String(LIMIT));\n    queryString = query.toString();\n    dispatch(getFestivalListMoreAsync.request(queryString));\n  };\n\n  const handleCategory = (key: string) => (\n    e: React.ChangeEvent<HTMLSelectElement>,\n  ) => {\n    e.target.value === 'all'\n      ? query.delete(key)\n      : query.set(key, e.target.value);\n    history.push(`/festival/list?${query.toString()}`);\n  };\n\n  const handleSearch = (e: React.KeyboardEvent<HTMLInputElement>) => {\n    if (e.key === 'Enter') {\n      history.push(`/festival/list?search=${inputQuery.search}`);\n    }\n  };\n\n  const handleInputSearch = (e: React.ChangeEvent<HTMLInputElement>) => {\n    setInputQuery((state) => ({\n      ...state,\n      search: e.target.value,\n    }));\n  };\n\n  return (\n    <>\n      <BackgorundImage />\n\n      <ListPresenter>\n        <FestivalCategory>\n          <FestivalCategoryHead> Festival List </FestivalCategoryHead>\n          <SearchBar\n            type={'text'}\n            value={inputQuery.search ? inputQuery.search : ''}\n            placeholder=\"TYPE TO SEARCH\"\n            onChange={handleInputSearch}\n            onKeyPress={handleSearch}\n          />\n          {festivalCategory.data &&\n            festivalCategory.data\n              .filter((item, index) => index < listNumber)\n              .map((item) => (\n                <Link key={`C${item._id}`} to={`/festival/detail/${item._id}`}>\n                  <FestivalCategoryContent>{item.name}</FestivalCategoryContent>\n                </Link>\n              ))}\n          {festivalCategory.data && festivalCategory.data.length > listNumber && (\n            <MoreButton\n              onClick={() => {\n                setListNumber((state) => state + 10);\n              }}\n            >\n              <MoreButtonText>More...</MoreButtonText>\n            </MoreButton>\n          )}\n        </FestivalCategory>\n        <ContentsSection>\n          <CategorySection>\n            <CountryCategory\n              value={inputQuery.countryId ? inputQuery.countryId : 'all'}\n              onChange={handleCategory('countryId')}\n            >\n              <CountryCategoryContent key={`allCountry`} value={`all`}>\n                All Country Ca